<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DemoChart">
        <attr name="manColor" format="color"></attr>
        <attr name="womanColor" format="color"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="xAxisLength" format="integer"></attr>
        <attr name="xAxisScale" format="enum">
            <enum name="billion" value="1000000000"></enum>
            <enum name="million" value="1000000"></enum>
            <enum name="thousand" value="1000"></enum>
        </attr>
    </declare-styleable>

</resources>